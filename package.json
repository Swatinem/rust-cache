  },
  "homepage": "https://github.com/Swatinem/rust-cache#readme",
  "dependencies": {
    "@actions/cache": "^3.0.4",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2"
  },
  "devDependencies": {
